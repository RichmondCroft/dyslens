{
  "name": "dyslens",
  "displayName": "Dyslens",
  "version": "1.0.6",
  "description": "Dyslens is making the web more accessible for people who have different reading needs and suffering with dyslexia.",
  "author": "RichmondCroft",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "e2e": "jest --detectOpenHandles"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.11",
    "lodash": "^4.17.21",
    "plasmo": "0.46.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@plasmohq/prettier-plugin-sort-imports": "1.1.1",
    "@types/chrome": "0.0.193",
    "@types/jest": "^28.1.8",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "babel-jest": "^29.0.1",
    "jest": "^29.0.1",
    "prettier": "2.7.1",
    "puppeteer": "^16.2.0",
    "typescript": "4.7.4"
  },
  "manifest": {
    "web_accessible_resources": [
      {
        "resources": [
          "assets/**"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "permissions": [
      "storage",
      "tts"
    ]
  }
}